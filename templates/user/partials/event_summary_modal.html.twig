<dialog class="eventModal eventSummaryModal">
  <div class="eventModal-content">
    <button class="closeButton"><i class="ph ph-x"></i></button>
    <h2 class="modalTitle"></h2>
    {% if is_registered %}
      <p>Vous êtes inscrit à cet événement.</p>
    {% endif %}
    <p class="modalRegisteredVolunteers"></p>
    <ul class="modalVolunteerList"></ul>
    <p class="modalDescription"></p>
    <p class="modalLocation"></p>
    <p class="modalTime"></p>
  </div>
</dialog>
